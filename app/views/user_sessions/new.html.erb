<h1>Log in</h1>

<div class="form-wrapper">
  <%= form_tag user_sessions_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag "Log in", :class => "button" %>
  <% end %>
</div>
  